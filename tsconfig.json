{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": false,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "types": [],
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["src"],
  "references": [
    { "path": "tsconfig.server.json" }
  ]
}
